// RUN: fooc %s -emit=llvm | FileCheck %s

func @foo(%arg0: f64) -> f64 {
  %0 = constant 1.0 : f64
  %1 = addf %arg0, %0 : f64
  return %1 : f64
}
// CHECK-LABEL: define double @foo(double %0) !dbg !3 {
// CHECK-NEXT:   %2 = fadd double %0, 1.000000e+00, !dbg !7
// CHECK-NEXT:   ret double %2, !dbg !9
// CHECK-NEXT: }
